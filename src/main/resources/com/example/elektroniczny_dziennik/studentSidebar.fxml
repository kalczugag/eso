<AnchorPane prefHeight="440.0" prefWidth="192.0" stylesheets="@styles/sidebar.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox alignment="TOP_CENTER" prefHeight="440.0" prefWidth="192.0" spacing="8.0" AnchorPane.rightAnchor="0.0">
            <children>
                <Button id="dashboardButton" fx:id="dashboardButton" alignment="BASELINE_LEFT" graphicTextGap="15.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#showDashboard" styleClass="sidebar-button" text="Pulpit" />
                <Button id="gradesButton" fx:id="gradesButton" alignment="BASELINE_LEFT" graphicTextGap="15.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#showGrades" styleClass="sidebar-button" text="Moje Oceny" />
            </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
        </VBox>
    </children>
</AnchorPane>